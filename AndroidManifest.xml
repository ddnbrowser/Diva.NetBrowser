<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="net.diva.browser"
      android:versionCode="393" android:versionName="3.93">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
    <application
        android:label="@string/app_name"
        android:icon="@drawable/icon"
        android:name=".DdN"
        android:theme="@style/Theme.DdN"
        >
        <activity android:name=".MainActivity"
                  android:label="@string/app_name"
                  android:screenOrientation="user"
                  android:configChanges="keyboardHidden|orientation"
                  android:uiOptions="splitActionBarWhenNarrow"
                  android:theme="@android:style/Theme.Black"
                  >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyListEditActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".MusicDetailActivity" android:theme="@style/Theme.DdN.Translucent.NoTitleBar"></activity>
        <activity
            android:name=".WebBrowseActivity"
            android:configChanges="keyboardHidden|orientation"
            android:uiOptions="splitActionBarWhenNarrow"
            />
        <activity android:name=".CommonConfigActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".SettingsActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".settings.TitleListActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".settings.DecorTitlesActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".settings.ModuleListActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".settings.SkinListActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".settings.SEListActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".settings.ShopActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".settings.TabSortActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".ticket.DecorPrizeActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".ticket.SkinPrizeActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".history.HistoryDetailActivity" android:theme="@android:style/Theme.Black"></activity>
        <activity android:name=".DdNMapActivity" android:theme="@android:style/Theme.Black" android:label="@string/app_name" />
        <receiver android:name=".DdNBrowserReceiver" android:exported="false">
            <intent-filter>
                <action android:name="net.diva.browser.action.DOWNLOAD_RANKING"></action>
                <action android:name="net.diva.browser.action.DOWNLOAD_HISTORY"></action>
            </intent-filter>
        </receiver>
        <service android:name=".DownloadRankingService"></service>
        <service android:name=".history.DownloadHistoryService"></service>
        <uses-library android:name="com.google.android.maps"/>
    </application>
</manifest>
