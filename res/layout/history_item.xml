<?xml version="1.0" encoding="utf-8"?>
<view xmlns:android="http://schemas.android.com/apk/res/android"
	class="net.diva.browser.util.CheckedFrameLayout"
	android:layout_height="wrap_content"
	android:layout_width="fill_parent"
	android:paddingRight="6dip"
	>
<RelativeLayout
	android:layout_height="wrap_content"
	android:layout_width="fill_parent"
	>
	<LinearLayout
		android:id="@+id/date_status"
		android:orientation="horizontal"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		>
	    <TextView
			android:id="@+id/play_date"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:maxLines="1"
			android:ellipsize="end"
			android:textColor="#00FFFF"
			android:text="[11/12/23 11:12]"
			/>
	    <TextView
			android:id="@+id/rank"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="6dip"
			android:maxLines="1"
			android:ellipsize="end"
			android:text="EXTREME"
			/>
	    <TextView
			android:id="@+id/clear_status"
			android:layout_width="0dip"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:maxLines="1"
			android:ellipsize="end"
			android:gravity="right"
			android:text="MISS×TAKE"
			/>

	</LinearLayout>
	<TextView
		android:id="@+id/music_title"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:maxLines="1"
		android:ellipsize="end"
		android:layout_below="@+id/date_status"
		android:text="ワールドイズマイン"
		/>
	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_below="@+id/music_title"
		>
		<ImageView
			android:id="@+id/lock"
			android:layout_width="24dip"
			android:layout_height="17dip"
			android:maxLines="1"
			android:ellipsize="end"
			android:src="@android:drawable/ic_lock_lock"
		/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:maxLines="1"
			android:ellipsize="end"
			android:text="スコア:"
			/>
		<TextView
			android:id="@+id/score"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:layout_marginRight="6dip"
			android:maxLines="1"
			android:ellipsize="end"
			android:gravity="right"
			android:text="378260pts"
			/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:maxLines="1"
			android:ellipsize="end"
			android:text="達成率:"
			/>
		<TextView
			android:id="@+id/achievement"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_weight="1"
			android:maxLines="1"
			android:ellipsize="end"
			android:gravity="right"
			android:text="89.91%"
			/>
	</LinearLayout>
</RelativeLayout>
</view>
